@page
@model FinalProject.Pages.Admin.LoginModel
@{
}

<form method="post">
    <div class="form-group mb-1">
        <label class="form-label" asp-for=" Username">Username</label>
        <input class="form-control" asp-for="Username" placeholder="Enter your username" />
    </div>
    <div class="form-group mb-1">
        <label class="form-label" asp-for="Password">Password</label>
        <input class="form-control" type="password" asp-for="Password" placeholder="Enter your password" />
    </div>
    <button class="btn btn-primary" type="submit">Login</button>
</form>
